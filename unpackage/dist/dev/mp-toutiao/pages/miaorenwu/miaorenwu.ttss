}
.demo3 {
	width: 4px;
	height: 4px;
	border-radius: 2px;
	background: #68b2ce;
	position: absolute;
	animation: demo3 linear 0.8s infinite;
	-webkit-animation: demo3 linear 0.8s infinite;
}
.demo3:nth-child(1) {
	left: 15px;
	top: 2px;
	-webkit-animation-delay: 0s;
	        animation-delay: 0s;
}
.demo3:nth-child(2) {
	left: 24px;
	top: 6px;
	-webkit-animation-delay: 0.1s;
	        animation-delay: 0.1s;
}
.demo3:nth-child(3) {
	left: 27px;
	top: 15px;
	-webkit-animation-delay: 0.1s;
	        animation-delay: 0.1s;
}
.demo3:nth-child(4) {
	left: 23px;
	top: 24px;
	-webkit-animation-delay: 0.2s;
	        animation-delay: 0.2s;
}
.demo3:nth-child(5) {
	left: 15px;
	top: 26px;
	-webkit-animation-delay: 0.4s;
	        animation-delay: 0.4s;
}
.demo3:nth-child(6) {
	left: 6px;
	top: 23px;
	-webkit-animation-delay: 0.5s;
	        animation-delay: 0.5s;
}
.demo3:nth-child(7) {
	left: 2px;
	top: 15px;
	-webkit-animation-delay: 0.6s;
	        animation-delay: 0.6s;
}
.demo3:nth-child(8) {
	left: 6px;
	top: 6px;
	-webkit-animation-delay: 0.7s;
	        animation-delay: 0.7s;
}
@keyframes demo3 {
0%,
	40%,
	100% {
		-webkit-transform: scale(1);
		        transform: scale(1);
}
20% {
		-webkit-transform: scale(3);
		        transform: scale(3);
}
}
@-webkit-keyframes demo3 {
0%,
	40%,
	100% {
		-webkit-transform: scale(1);
		        transform: scale(1);
}
20% {
		-webkit-transform: scale(3);
		        transform: scale(3);
}
}

